  "classification_details": {
    "type_1": {
      "definition": "Incomplete seal at graft attachment sites",
      "subtypes": {
        "type_1a": "Proximal attachment site leak",
        "type_1b": "Distal attachment site leak",
        "type_1c": "Iliac occluder plug leak"
      },
      "incidence": "8-10% at initial procedure, <5% persistent",
      "pressure": "systemic_arterial_pressure",
      "urgency": "urgent_intervention_required",
      "risk_level": "high",
      "typical_management": "Additional stent-graft, proximal/distal extension, conversion to open repair"
    },
    "type_2": {
      "definition": "Retrograde flow through aortic branch vessels",
      "source_vessels": [
        "lumbar_arteries",
        "inferior_mesenteric_artery",
        "accessory_renal_arteries",
        "median_sacral_artery"
      ],
      "incidence": "10-45% of EVAR cases",
      "pressure": "collateral_circulation_pressure",
      "urgency": "observation_unless_sac_expansion",
      "risk_level": "moderate",
      "spontaneous_resolution": "~50% within 6 months",
      "typical_management": "Observation if sac stable, embolization if persistent with sac growth"
    },
    "type_3": {
      "definition": "Structural failure of stent-graft",
      "subtypes": {
        "type_3a": "Junctional separation between components",
        "type_3b": "Fabric tear or graft disruption"
      },
      "incidence": "1-5% with newer generation devices",
      "pressure": "systemic_arterial_pressure",
      "urgency": "urgent_intervention_required",
      "risk_level": "high",
      "typical_management": "Additional stent-graft to bridge defect, extension, or conversion"
    },
    "type_4": {
      "definition": "Flow through porous graft fabric",
      "incidence": "rare with modern grafts (<1%)",
      "pressure": "transmural_through_fabric",
      "urgency": "usually_not_clinically_significant",
      "risk_level": "low",
      "temporal_pattern": "Typically occurs immediately post-op, resolves as graft incorporates",
      "typical_management": "Observation, rarely requires intervention"
    },
    "type_5": {
      "definition": "Endotension - sac expansion without identifiable leak on imaging",
      "incidence": "1-5%, controversial entity",
      "pressure": "mechanism_unclear",
      "urgency": "intervention_if_significant_expansion",
      "risk_level": "uncertain",
      "controversy": "Debate about whether represents undetected leak vs. true entity",
      "typical_management": "Enhanced imaging techniques, possible conversion to open repair if expansion continues"
    }
  },
  "clinical_decision_framework": {
    "immediate_intervention_required": [
      "Type 1 (any subtype)",
      "Type 3 (any subtype)"
    ],
    "observation_appropriate": [
      "Type 2 with stable or decreasing sac size",
      "Type 4 (most cases)",
      "Type 5 with minimal sac growth"
    ],
    "delayed_intervention_considered": [
      "Type 2 with sac expansion >5mm",
      "Type 2 with sac expansion >10mm absolute",
      "Type 5 with significant ongoing expansion"
    ]
  },
  "imaging_characteristics": {
    "CT_angiography_findings": {
      "type_1": "Contrast in sac adjacent to attachment zones",
      "type_2": "Contrast in sac with identifiable feeding/draining vessels",
      "type_3": "Contrast between graft components or through fabric tear",
      "type_4": "Diffuse contrast blush through graft wall",
      "type_5": "No contrast in sac but sac expansion documented"
    },
    "temporal_imaging_patterns": {
      "type_1": "Present immediately, persists",
      "type_2": "May appear late, can resolve",
      "type_3": "Can occur anytime, indicates device failure",
      "type_4": "Immediate, typically resolves",
      "type_5": "Progressive sac growth without visible leak"
    }
  },
