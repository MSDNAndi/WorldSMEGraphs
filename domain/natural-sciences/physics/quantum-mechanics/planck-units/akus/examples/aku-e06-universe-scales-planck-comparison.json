  "content": {
    "title": "Universe Scales vs Planck Scale: From Quantum to Cosmos",
    "abstract": "A comprehensive comparison of physical scales from the Planck length (10⁻³⁵ m) to the observable universe (10²⁶ m), spanning 61 orders of magnitude. This example illustrates why the Planck scale is truly 'fundamental' and demonstrates the vast hierarchies in nature.",
    "problem_statement": "How does the Planck length compare to familiar scales—atoms, cells, humans, planets, and the observable universe? How many Planck lengths fit across the universe?",
    "given_values": {
      "planck_length": "ℓ_P = 1.616 × 10⁻³⁵ m",
      "planck_time": "t_P = 5.391 × 10⁻⁴⁴ s",
      "proton_diameter": "d_p ≈ 1.7 × 10⁻¹⁵ m",
      "hydrogen_atom": "a_0 = 5.3 × 10⁻¹¹ m (Bohr radius)",
      "human_hair": "d_hair ≈ 75 × 10⁻⁶ m = 75 μm",
      "human_height": "h_human ≈ 1.7 m",
      "earth_diameter": "D_Earth = 1.27 × 10⁷ m",
      "sun_diameter": "D_Sun = 1.39 × 10⁹ m",
      "light_year": "ly = 9.46 × 10¹⁵ m",
      "milky_way": "D_MW ≈ 10⁵ ly ≈ 10²¹ m",
      "observable_universe": "D_obs ≈ 8.8 × 10²⁶ m (diameter)"
    },
    "solution": {
      "step_1": {
        "title": "Planck Length to Proton",
        "calculation": "d_p/ℓ_P = (1.7×10⁻¹⁵)/(1.6×10⁻³⁵) ≈ 10²⁰",
        "interpretation": "A proton is 10²⁰ Planck lengths across!",
        "analogy": "If Planck length were 1 mm, a proton would span our entire galaxy"
      },
      "step_2": {
        "title": "Planck Length to Atom",
        "calculation": "a_0/ℓ_P = (5.3×10⁻¹¹)/(1.6×10⁻³⁵) ≈ 3.3 × 10²⁴",
        "interpretation": "A hydrogen atom is over 10²⁴ Planck lengths across",
        "analogy": "More Planck lengths in an atom than atoms in an apple"
      },
      "step_3": {
        "title": "Planck Length to Human",
        "calculation": "h/ℓ_P = 1.7/(1.6×10⁻³⁵) ≈ 10³⁵",
        "interpretation": "A human is 10³⁵ Planck lengths tall",
        "geometric_mean": "Remarkably, humans are roughly at the geometric mean: √(ℓ_P × D_obs) ≈ √(10⁻³⁵ × 10²⁷) = 10⁻⁴ m = 0.1 mm"
      },
      "step_4": {
        "title": "Planck Length to Observable Universe",
        "calculation": "D_obs/ℓ_P = (8.8×10²⁶)/(1.6×10⁻³⁵) ≈ 5.4 × 10⁶¹",
        "interpretation": "The observable universe is about 10⁶¹ Planck lengths across!",
        "significance": "This is the full range of meaningful length scales in physics"
      },
      "step_5": {
        "title": "Planck Time to Age of Universe",
        "calculation": "t_universe/t_P = (4.35×10¹⁷ s)/(5.4×10⁻⁴⁴ s) ≈ 8 × 10⁶⁰",
        "interpretation": "The universe is about 10⁶⁰ Planck times old",
        "note": "t_universe = 13.8 billion years = 4.35 × 10¹⁷ seconds"
      },
