  "content": {
    "statement": "aku-f01-dimensional-analysis is a fundamental concept in Natural-Sciences Physics Quantum-Mechanics Planck-Units that provides essential knowledge for understanding related topics and clinical/practical applications.",
    "explanation": {
      "intuition": "Understanding aku-f01-dimensional-analysis helps build foundational knowledge. The concept connects to broader principles in the field.",
      "key_insight": "The key aspects of aku-f01-dimensional-analysis include core principles, common applications, and integration with related concepts.",
      "technical_details": "Technical details involve specific mechanisms, measurements, and evidence-based applications of aku-f01-dimensional-analysis."
    },
    "technique": {
      "general_method": {
        "step_1": {
          "title": "Express target quantity in dimensions",
          "description": "Write the physical quantity you want to derive in terms of fundamental dimensions [M, L, T, K, A]",
          "example": "For length: [X] = [L], For time: [X] = [T], For mass: [X] = [M]"
        },
        "step_2": {
          "title": "List available fundamental constants with dimensions",
          "description": "Identify which fundamental constants are available and their dimensional formulas",
          "constants": {
            "c": {
              "name": "speed of light",
              "dimensions": "[L/T] = [L\u00b9T\u207b\u00b9]",
              "value": "299792458 m/s (exact)"
            },
            "hbar": {
              "name": "reduced Planck constant",
              "dimensions": "[ML\u00b2/T] = [M\u00b9L\u00b2T\u207b\u00b9]",
              "value": "1.054571817... \u00d7 10\u207b\u00b3\u2074 J\u00b7s (exact)"
            },
            "G": {
              "name": "gravitational constant",
              "dimensions": "[L\u00b3/MT\u00b2] = [M\u207b\u00b9L\u00b3T\u207b\u00b2]",
              "value": "6.67430(15) \u00d7 10\u207b\u00b9\u00b9 m\u00b3/(kg\u00b7s\u00b2)"
            },
            "k_B": {
              "name": "Boltzmann constant",
              "dimensions": "[ML\u00b2/T\u00b2K] = [M\u00b9L\u00b2T\u207b\u00b2K\u207b\u00b9]",
              "value": "1.380649 \u00d7 10\u207b\u00b2\u00b3 J/K (exact)"
            },
            "epsilon_0": {
              "name": "electric constant",
              "dimensions": "[M\u207b\u00b9L\u207b\u00b3T\u2074A\u00b2]",
              "value": "8.8541878128(13) \u00d7 10\u207b\u00b9\u00b2 F/m"
            }
          }
        },
        "step_3": {
          "title": "Set up the ansatz",
          "description": "Assume the quantity has form X ~ c^a \u00b7 \u210f^b \u00b7 G^d \u00b7 k_B^e \u00b7 \u03b5\u2080^f",
          "note": "For most Planck units, we only need c, \u210f, G (so e = f = 0)"
        },
        "step_4": {
          "title": "Write dimensional equation",
          "description": "Substitute dimensions: [X] = [c]^a \u00b7 [\u210f]^b \u00b7 [G]^d",
          "expansion": "[M^\u03b1 L^\u03b2 T^\u03b3] = [L\u00b9T\u207b\u00b9]^a \u00b7 [M\u00b9L\u00b2T\u207b\u00b9]^b \u00b7 [M\u207b\u00b9L\u00b3T\u207b\u00b2]^d"
        },
        "step_5": {
          "title": "Equate exponents for each dimension",
          "description": "Create system of equations by matching powers of M, L, T",
          "system": {
            "mass_M": "\u03b1 = b - d",
            "length_L": "\u03b2 = a + 2b + 3d",
            "time_T": "\u03b3 = -a - b - 2d"
          }
        },
        "step_6": {
          "title": "Solve the system",
          "description": "Solve linear equations for unknowns a, b, d given target (\u03b1, \u03b2, \u03b3)",
          "method": "Use substitution, elimination, or matrix methods"
        }
      },
      "worked_examples": {
        "example_1_planck_length": {
          "target": "Length [L\u00b9]",
          "dimensions": "\u03b1 = 0, \u03b2 = 1, \u03b3 = 0",
          "setup": "[L\u00b9] = [L\u00b9T\u207b\u00b9]^a \u00b7 [M\u00b9L\u00b2T\u207b\u00b9]^b \u00b7 [M\u207b\u00b9L\u00b3T\u207b\u00b2]^d",
          "equations": {
            "M": "0 = b - d \u2192 b = d",
            "L": "1 = a + 2b + 3d",
            "T": "0 = -a - b - 2d"
          },
          "solution_step_1": "From T equation: a = -b - 2d = -d - 2d = -3d",
          "solution_step_2": "Substitute into L equation: 1 = (-3d) + 2d + 3d = 2d",
          "solution_step_3": "Therefore: d = 1/2, b = 1/2, a = -3/2",
          "result": "\u2113\u209a = c^(-3/2) \u00b7 \u210f^(1/2) \u00b7 G^(1/2) = \u221a(\u210fG/c\u00b3)",
          "latex": "\\ell_P = \\sqrt{\\frac{\\hbar G}{c^3}}",
          "numerical_value": "1.616255(18) \u00d7 10\u207b\u00b3\u2075 m"
        },
        "example_2_planck_time": {
          "target": "Time [T\u00b9]",
          "dimensions": "\u03b1 = 0, \u03b2 = 0, \u03b3 = 1",
          "setup": "[T\u00b9] = [L\u00b9T\u207b\u00b9]^a \u00b7 [M\u00b9L\u00b2T\u207b\u00b9]^b \u00b7 [M\u207b\u00b9L\u00b3T\u207b\u00b2]^d",
          "equations": {
            "M": "0 = b - d \u2192 b = d",
            "L": "0 = a + 2b + 3d",
            "T": "1 = -a - b - 2d"
          },
          "solution_step_1": "From M: b = d",
          "solution_step_2": "From L: a = -2b - 3d = -2d - 3d = -5d",
          "solution_step_3": "From T: 1 = -(-5d) - d - 2d = 5d - d - 2d = 2d",
          "solution_step_4": "Therefore: d = 1/2, b = 1/2, a = -5/2",
          "result": "t\u209a = c^(-5/2) \u00b7 \u210f^(1/2) \u00b7 G^(1/2) = \u221a(\u210fG/c\u2075)",
          "latex": "t_P = \\sqrt{\\frac{\\hbar G}{c^5}}",
          "numerical_value": "5.391247(60) \u00d7 10\u207b\u2074\u2074 s",
          "verification": "t\u209a = \u2113\u209a/c as expected"
        },
        "example_3_planck_mass": {
          "target": "Mass [M\u00b9]",
          "dimensions": "\u03b1 = 1, \u03b2 = 0, \u03b3 = 0",
          "setup": "[M\u00b9] = [L\u00b9T\u207b\u00b9]^a \u00b7 [M\u00b9L\u00b2T\u207b\u00b9]^b \u00b7 [M\u207b\u00b9L\u00b3T\u207b\u00b2]^d",
          "equations": {
            "M": "1 = b - d",
            "L": "0 = a + 2b + 3d",
            "T": "0 = -a - b - 2d"
          },
          "solution_step_1": "From T: a = -b - 2d",
          "solution_step_2": "Substitute into L: 0 = (-b - 2d) + 2b + 3d = b + d",
          "solution_step_3": "Therefore: d = -b",
          "solution_step_4": "From M: 1 = b - d = b - (-b) = 2b \u2192 b = 1/2",
          "solution_step_5": "Therefore: b = 1/2, d = -1/2, a = -1/2 - 2(-1/2) = -1/2 + 1 = 1/2",
          "result": "m\u209a = c^(1/2) \u00b7 \u210f^(1/2) \u00b7 G^(-1/2) = \u221a(\u210fc/G)",
          "latex": "m_P = \\sqrt{\\frac{\\hbar c}{G}}",
          "numerical_value": "2.176434(24) \u00d7 10\u207b\u2078 kg"
        }
      },
      "complete_table": {
        "description": "All Planck units with their exponents",
        "table": [
          {
            "quantity": "Planck length",
            "symbol": "\u2113\u209a",
            "exponents": {
              "c": -1.5,
              "hbar": 0.5,
              "G": 0.5
            },
            "formula": "\u221a(\u210fG/c\u00b3)",
            "value": "1.616 \u00d7 10\u207b\u00b3\u2075 m"
          },
          {
            "quantity": "Planck time",
            "symbol": "t\u209a",
            "exponents": {
              "c": -2.5,
              "hbar": 0.5,
              "G": 0.5
            },
            "formula": "\u221a(\u210fG/c\u2075)",
            "value": "5.391 \u00d7 10\u207b\u2074\u2074 s"
          },
          {
            "quantity": "Planck mass",
            "symbol": "m\u209a",
            "exponents": {
              "c": 0.5,
              "hbar": 0.5,
              "G": -0.5
            },
            "formula": "\u221a(\u210fc/G)",
            "value": "2.176 \u00d7 10\u207b\u2078 kg"
          },
          {
            "quantity": "Planck energy",
            "symbol": "E\u209a",
            "exponents": {
              "c": 2.5,
              "hbar": 0.5,
              "G": -0.5
            },
            "formula": "\u221a(\u210fc\u2075/G)",
            "value": "1.956 \u00d7 10\u2079 J"
          },
          {
            "quantity": "Planck temperature",
            "symbol": "T\u209a",
            "exponents": {
              "c": 2.5,
              "hbar": 0.5,
              "G": -0.5,
              "k_B": -1
            },
            "formula": "\u221a(\u210fc\u2075/Gk_B\u00b2)",
            "value": "1.417 \u00d7 10\u00b3\u00b2 K"
          },
          {
            "quantity": "Planck charge",
            "symbol": "q\u209a",
            "exponents": {
              "hbar": 0.5,
              "c": 0.5,
              "epsilon_0": 0.5
            },
            "formula": "\u221a(4\u03c0\u03b5\u2080\u210fc)",
            "value": "1.876 \u00d7 10\u207b\u00b9\u2078 C"
          }
        ]
      }
    },
