  "hemodynamic_principles": {
    "pressure_dynamics": {
      "systemic_arterial_pressure": "100-140 mmHg (normal)",
      "collateral_circuit_pressure": "30-60 mmHg (estimated)",
      "sac_pressure_type2": "Variable, typically 30-70 mmHg (lower than systemic)",
      "sac_pressure_type1or3": "Near systemic pressure (90-130 mmHg)",
      "clinical_significance": "Lower pressure in Type 2 explains lower rupture risk"
    },
    "flow_patterns": {
      "flow_through_lesion": {
        "description": "Inflow vessel + outflow vessel creating circuit",
        "example": "IMA inflow → lumbar outflow",
        "spontaneous_thrombosis": "less likely",
        "intervention_complexity": "requires embolization of both vessels"
      },
      "flow_in_lesion": {
        "description": "Single inflow vessel with stagnant sac",
        "example": "Single lumbar artery inflow only",
        "spontaneous_thrombosis": "more likely",
        "intervention_complexity": "single vessel embolization may suffice"
      }
    },
    "factors_affecting_persistence": [
      "Number of patent branch vessels",
