  "clinical_features": {
    "renal_fmd": {
      "presentation": [
        "Hypertension (often before age 35)",
        "Resistant hypertension",
        "Renal insufficiency (rare)"
      ],
      "diagnosis": [
        "CTA or MRA",
        "Classic 'string of beads'",
        "Mid/distal renal artery location"
      ],
      "treatment": [
        "Balloon angioplasty (not stenting)",
        "BP control"
      ]
    },
    "cervicocephalic_fmd": {
      "presentation": [
        "TIA or stroke",
        "Carotid/vertebral dissection",
        "Headache",
        "Pulsatile tinnitus",
        "Horner syndrome"
      ],
      "associated_findings": [
        "Intracranial aneurysm (7-15%)",
        "Tortuous vessels"
      ],
      "treatment": [
        "Antiplatelet therapy",
        "BP control",
        "Dissection: anticoagulation/antiplatelet"
      ]
    },
    "screening_recommendations": {
      "renal_fmd": "Screen brain for intracranial aneurysm with MRA",
      "carotid_fmd": "Screen brain for aneurysm; consider renal duplex"
    }
