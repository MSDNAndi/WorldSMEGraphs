  "content": {
    "statement": "aaa-009-evar-vs-open is a fundamental concept in Health-Sciences Medicine Surgery Vascular Pathology Aaa that provides essential knowledge for understanding related topics and clinical/practical applications.",
    "explanation": {
      "intuition": "Understanding aaa-009-evar-vs-open helps build foundational knowledge. The concept connects to broader principles in the field.",
      "key_insight": "The key aspects of aaa-009-evar-vs-open include core principles, common applications, and integration with related concepts.",
      "technical_details": "Technical details involve specific mechanisms, measurements, and evidence-based applications of aaa-009-evar-vs-open."
    },
    "definitions_glossary": {
      "evar": "Endovascular Aortic Repair - stent-graft placement via femoral access",
      "open_repair": "Traditional surgery with aortic clamping and graft interposition",
      "proximal_neck": "Normal aorta between renal arteries and aneurysm",
      "reintervention": "Additional procedures to address complications or failures",
      "surveillance": "Regular imaging follow-up to detect complications"
    }
  },
  "clinical_features": {
    "comparison_table": {
      "perioperative_mortality": {
        "evar": "1-2%",
        "open": "4-5%"
      },
      "hospital_stay": {
        "evar": "2-3 days",
        "open": "7-10 days"
      },
      "recovery_time": {
        "evar": "1-2 weeks",
        "open": "4-6 weeks"
      },
      "five_year_reintervention": {
        "evar": "20-30%",
        "open": "5-10%"
      },
      "aneurysm_related_mortality_5y": {
        "evar": "3-5%",
        "open": "2-3%"
      },
      "all_cause_mortality_5y": {
        "evar": "25-30%",
        "open": "25-30%"
      },
      "surveillance_required": {
        "evar": "Yes, lifelong",
        "open": "No (unless indication)"
      }
    },
    "evar_anatomic_requirements": [
      "Proximal neck length \u226510-15 mm",
      "Proximal neck diameter \u226430-32 mm",
      "Proximal neck angulation <60\u00b0",
      "Iliac diameter \u22657 mm for access",
      "Iliac tortuosity <90\u00b0",
      "Adequate distal landing zones"
    ],
    "open_repair_preferred": [
      "Young/fit patients with long life expectancy",
      "EVAR-ineligible anatomy",
      "Connective tissue disease (Marfan, Ehlers-Danlos)",
      "Mycotic or inflammatory aneurysm",
      "Need for renal/mesenteric revascularization",
      "Concern about compliance with surveillance"
    ],
    "evar_preferred": [
      "Older patients (>70-80 years)",
      "High operative risk (cardiac, pulmonary)",
      "Hostile abdomen (prior surgery, radiation)",
      "Favorable anatomy",
      "Patient preference for minimally invasive"
    ]
  },
