  "content": {
    "identifier": "FMD-004",
    "title": "Carotid and Vertebral Fibromuscular Dysplasia",
    "definition": "Fibromuscular dysplasia affecting the cervical internal carotid and vertebral arteries, causing arterial stenosis, aneurysm, and dissection with risk of stroke and TIA.",
    "concepts": [
      {
        "concept_id": "location",
        "term": "Anatomic Distribution",
        "definition": "Where cervical FMD typically occurs.",
        "clinical_features": {
          "carotid": "Mid to distal cervical ICA (C1-C2 level typical)",
          "vertebral": "V2-V3 segments most common",
          "bilateral": "Bilateral in 60-85% for carotid involvement",
          "spares_origin": "Unlike atherosclerosis, spares carotid bulb"
        }
      },
      {
        "concept_id": "presentation",
        "term": "Clinical Presentation",
        "definition": "How cervical FMD presents.",
        "clinical_features": {
          "asymptomatic": "Many found incidentally on imaging",
          "tia_stroke": "Cerebrovascular events from embolism or dissection",
          "pulsatile_tinnitus": "Whooshing sound from turbulent flow",
          "headache": "Migraine or tension-type headaches common",
          "dissection": "May present with acute dissection"
        }
      },
      {
        "concept_id": "complications",
        "term": "Associated Findings",
        "definition": "Complications and associations of cervical FMD.",
        "clinical_features": {
          "aneurysm": "Intracranial aneurysms in 7-14%",
          "dissection": "Increased risk of spontaneous dissection",
          "multifocal": "Often have renal or iliac FMD also",
          "screening": "Screen for intracranial aneurysms recommended"
        }
      },
      {
        "concept_id": "management",
        "term": "Management",
        "definition": "Treatment approach for cervical FMD.",
        "clinical_features": {
          "asymptomatic": "Antiplatelet therapy, risk factor control",
          "symptomatic_stenosis": "Angioplasty may be considered",
          "dissection": "Anticoagulation vs antiplatelet (similar to spontaneous)",
          "aneurysm": "Treat intracranial aneurysms per neurosurgical standards"
        }
      }
    ],
    "clinical_features": {
      "natural_history": "Generally benign if no dissection or stroke",
      "follow_up": "Annual imaging surveillance for progression"
    }
  },
