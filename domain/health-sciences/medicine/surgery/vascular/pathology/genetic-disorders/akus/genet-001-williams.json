  "content": {
    "title": "Williams Syndrome Vascular Manifestations",
    "description": "Vascular abnormalities in Williams syndrome caused by elastin gene deletion",
    "key_concepts": [
      {
        "concept": "Genetics",
        "details": "Microdeletion at 7q11.23 including the ELN (elastin) gene"
      },
      {
        "concept": "Supravalvular Aortic Stenosis",
        "details": "Most common vascular manifestation; occurs in 45-75% of patients"
      },
      {
        "concept": "Pulmonary Artery Stenosis",
        "details": "Bilateral peripheral pulmonary artery stenosis common"
      },
      {
        "concept": "Arterial Stenoses",
        "details": "Can affect any artery; renal artery stenosis, coronary, cerebral vessels"
      }
    ],
    "clinical_significance": "Generalized arteriopathy requiring multi-vessel surveillance"
  },
  "provenance": {
    "primary_sources": ["Collins RT. Circ Res 2013"]
  }
}
