  "content": {
    "title": "NF1 Vasculopathy",
    "description": "Vascular manifestations of neurofibromatosis type 1",
    "key_concepts": [
      {
        "concept": "Renal Artery Involvement",
        "details": "Most common vascular manifestation; may cause renovascular hypertension"
      },
      {
        "concept": "Pathology",
        "details": "Intimal proliferation, medial thinning, aneurysm formation"
      },
      {
        "concept": "Coarctation-like Lesions",
        "details": "May develop abdominal aortic coarctation"
      },
      {
        "concept": "Surgical Challenges",
        "details": "Tissue fragility, potential involvement of surrounding structures by neurofibromas"
      }
    ],
    "clinical_significance": "Vasculopathy affects 0.4-6.4% of NF1 patients; may present with hypertension"
  },
  "provenance": {
    "primary_sources": ["Oderich GS et al. J Vasc Surg 2007"]
  }
}
