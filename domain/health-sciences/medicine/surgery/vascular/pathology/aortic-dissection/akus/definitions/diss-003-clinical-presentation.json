  "content": {
    "statement": "diss-003-clinical-presentation is a fundamental concept in Health-Sciences Medicine Surgery Vascular Pathology Aortic-Dissection that provides essential knowledge for understanding related topics and clinical/practical applications.",
    "explanation": {
      "intuition": "Understanding diss-003-clinical-presentation helps build foundational knowledge. The concept connects to broader principles in the field.",
      "key_insight": "The key aspects of diss-003-clinical-presentation include core principles, common applications, and integration with related concepts.",
      "technical_details": "Technical details involve specific mechanisms, measurements, and evidence-based applications of diss-003-clinical-presentation."
    },
    "definitions_glossary": {
      "pulse_deficit": "Absent or diminished pulse in one extremity compared to others",
      "malperfusion": "Inadequate blood supply to organs due to branch vessel compromise",
      "aortic_regurgitation": "Backflow through aortic valve, can be caused by Type A dissection",
      "dynamic_obstruction": "Intermittent branch vessel compression by mobile intimal flap",
      "static_obstruction": "Fixed branch vessel compromise by extending dissection"
    }
  },
  "clinical_features": {
    "symptoms": {
      "pain": {
        "frequency": "90%",
        "character": [
          "Sudden onset (85%)",
          "Severe/worst ever (90%)",
          "Tearing/ripping (50%)",
          "Sharp (60%)"
        ],
        "location_by_type": {
          "type_a": "Anterior chest, neck, jaw",
          "type_b": "Interscapular, back, abdomen"
        },
        "migration": "40% - suggests propagation"
      },
      "syncope": {
        "frequency": "15%",
        "causes": [
          "Hypotension",
          "Stroke",
          "Arrhythmia",
          "Pain"
        ]
      },
      "dyspnea": {
        "frequency": "25%",
        "causes": [
          "Heart failure from AR",
          "Hemothorax",
          "Pain"
        ]
      }
    },
    "signs": {
      "blood_pressure": {
        "hypertension": "50%",
        "hypotension": "10-15% (poor prognostic sign)",
        "bp_differential": "30% (>20 mmHg between arms)"
      },
      "pulse_deficit": {
        "frequency": "30%",
        "significance": "Indicates branch vessel involvement"
      },
      "aortic_regurgitation": {
        "frequency": "32% of Type A",
        "mechanism": "Annular dilation or leaflet detachment"
      },
      "neurologic_deficit": {
        "frequency": "15-20%",
        "types": [
          "Stroke",
          "Altered consciousness",
          "Paraplegia"
        ]
      }
    },
    "malperfusion_syndromes": [
      {
        "territory": "Cerebral (carotid/vertebral)",
        "manifestation": "Stroke, TIA, syncope, altered mental status",
        "frequency": "15-20%"
      },
      {
        "territory": "Coronary",
        "manifestation": "MI (usually RCA), chest pain, arrhythmia",
        "frequency": "1-5%"
      },
      {
        "territory": "Spinal cord",
        "manifestation": "Paraplegia, paraparesis",
        "frequency": "2-5%"
      },
      {
        "territory": "Mesenteric",
        "manifestation": "Abdominal pain, bloody stool, lactate elevation",
        "frequency": "3-5%"
      },
      {
        "territory": "Renal",
        "manifestation": "Oliguria, hematuria, flank pain",
        "frequency": "5-10%"
      },
      {
        "territory": "Lower extremity",
        "manifestation": "Acute limb ischemia, cool/pale/pulseless leg",
        "frequency": "10-15%"
      }
    ],
    "differential_diagnosis": [
      "Acute coronary syndrome",
      "Pulmonary embolism",
      "Pericarditis/tamponade",
      "Pneumothorax",
      "Musculoskeletal pain",
      "Esophageal rupture",
      "Acute pancreatitis"
    ]
  },
