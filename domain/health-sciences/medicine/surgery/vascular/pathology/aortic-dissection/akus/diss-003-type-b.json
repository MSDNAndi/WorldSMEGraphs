  "content": {
    "title": "Type B Aortic Dissection",
    "description": "Aortic dissection originating distal to the left subclavian artery, managed medically if uncomplicated or with TEVAR if complicated.",
    "presentation": {
      "typical": "Sudden interscapular/back pain",
      "complicated": "Malperfusion signs, refractory hypertension, rupture"
    },
    "uncomplicated_vs_complicated": {
      "uncomplicated": "No malperfusion, controlled hypertension, no rupture",
      "complicated": {
        "malperfusion": "Visceral, renal, or limb ischemia",
        "rupture": "Free or contained",
        "refractory_hypertension": "Despite maximum medical therapy",
        "refractory_pain": "Ongoing despite analgesics"
      }
    },
    "medical_management": {
      "goal": "Heart rate <60, SBP 100-120",
      "agents": ["Beta-blockers (first-line)", "Calcium channel blockers", "Vasodilators (after rate control)"],
      "monitoring": "ICU, serial imaging"
    },
    "intervention_indications": {
      "acute": "Complicated Type B",
      "chronic": "Aneurysmal degeneration",
      "preventive": "Increasing evidence for early TEVAR even in uncomplicated (INSTEAD-XL, ADSORB data)"
    },
    "treatment_options": {
      "tevar": "Primary therapy for complicated Type B",
      "open": "Reserved for connective tissue disorders, failed TEVAR",
      "hybrid": "Complex cases requiring arch or visceral revascularization"
    },
    "outcomes": {
      "uncomplicated_medical": "90% survival at 30 days",
      "complicated_tevar": "~90% technical success, 10-15% mortality"
    }
  },
