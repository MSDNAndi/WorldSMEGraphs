      "percent_stenosis": "(1 - Stenosis_Area/Reference_Area) \u00d7 100"
    },
    "ivus_findings": {
      "may_thurner": [
        "External compression of left common iliac vein",
        "Flattened elliptical shape",
        "Spur or web at compression point",
        "Normal wall architecture"
      ],
      "post_thrombotic": [
        "Irregular thickened vein wall",
        "Webs and synechiae (scar bands)",
        "Chronic organized thrombus",
        "Irregular luminal contour",
        "Recanalization channels"
      ],
      "external_compression": [
        "Smooth compression without wall abnormality",
        "May identify compressing structure"
      ]
    },
    "clinical_applications": {
      "diagnosis": [
        "Quantify stenosis severity (>50% area = significant)",
        "Differentiate compression from thrombotic disease",
        "Identify extent of disease"
      ],
      "treatment_guidance": [
        "Size stents to reference segment diameter",
        "Determine stent length (cover disease + margins)",
        "Assess landing zones",
        "Confirm stent apposition post-deployment"
      ],
      "quality_control": [
        "Document pre-intervention stenosis",
        "Confirm post-stent expansion",
        "Identify residual stenosis requiring further dilation"
      ]
    },
    "ivus_vs_venography": {
      "venography_limitations": [
        "Shows lumen silhouette only",
        "Cannot assess wall structure",
        "Underestimates compression by 30-50%",
        "Contrast dilution can obscure findings"
      ],
      "ivus_advantages": [
        "Direct cross-sectional area measurement",
        "Visualizes wall pathology",
        "Identifies webs and synechiae",
        "Accurate stent sizing",
        "Real-time procedural guidance"
      ]
    },
