  "content": {
    "title": "Parkes Weber Syndrome",
    "definition": "Congenital vascular malformation syndrome characterized by the combination of capillary malformation (port-wine stain), arteriovenous malformations/fistulas, and soft tissue/bony overgrowth of an affected extremity.",
    "differentiation_from_kts": {
      "parkes_weber": {
        "avms_present": "YES - high-flow arteriovenous shunting",
        "hemodynamics": "High-output state, warm extremity"
      },
      "klippel_trenaunay": {
        "avms_present": "NO - low-flow venous malformation only",
        "hemodynamics": "No AV shunting"
      }
    },
    "clinical_features": {
      "cutaneous": "Port-wine stain (capillary malformation)",
      "skeletal": "Limb hypertrophy (bone and soft tissue)",
      "vascular": {
        "avf_avms": "Multiple, diffuse arteriovenous connections",
        "hemodynamics": "Increased venous return, high-output failure possible"
      },
      "location": "Usually unilateral lower extremity"
    },
    "complications": {
      "cardiac": "High-output heart failure (rare but serious)",
      "limb": [
        "Pain",
        "Bleeding",
        "Ulceration",
        "Limb length discrepancy"
      ],
      "thrombotic": "VTE risk"
    },
    "diagnosis": {
      "clinical": "Characteristic triad",
      "imaging": {
        "doppler": "Shows high-flow AVMs",
        "mri_mra": "Defines extent of malformation",
        "angiography": "Details AV connections for treatment planning"
      }
    },
    "genetics": {
      "gene": "RASA1 mutations (some cases)",
      "inheritance": "Usually sporadic"
    },
    "treatment": {
      "approach": "Multidisciplinary, often palliative",
      "embolization": "Target AVMs to reduce flow",
      "surgery": "Limited role due to diffuse nature",
      "compression": "Symptomatic relief",
      "sirolimus": "Emerging therapy for vascular malformations"
    },
    "terminology": {
      "snomed_ct": "234139004",
      "icd_10": "Q87.2"
    }
  },
  "pedagogical": {
    "learning_objectives": [
      "Differentiate Parkes Weber from Klippel-Trenaunay",
      "Recognize high-flow vs low-flow malformation",
      "Understand treatment limitations"
    ],
    "clinical_pearls": [
      "PWS = KTS + AVMs (high flow)",
      "Warm extremity with bruit = Parkes Weber",
      "High-output heart failure possible in severe cases"
    ]
  },
