    "statement": "carotid-002-duplex-ultrasound is a fundamental concept in Health-Sciences Medicine Surgery Vascular Pathology Carotid-Disease that provides essential knowledge for understanding related topics and clinical/practical applications.",
    "explanation": {
      "intuition": "Understanding carotid-002-duplex-ultrasound helps build foundational knowledge. The concept connects to broader principles in the field.",
      "key_insight": "The key aspects of carotid-002-duplex-ultrasound include core principles, common applications, and integration with related concepts.",
      "technical_details": "Technical details involve specific mechanisms, measurements, and evidence-based applications of carotid-002-duplex-ultrasound."
    },
    "definitions_glossary": {
      "duplex_ultrasound": "Combined B-mode imaging and Doppler velocity measurement",
      "peak_systolic_velocity": "Maximum blood velocity during systole",
      "end_diastolic_velocity": "Blood velocity at end of diastole",
      "ica_cca_ratio": "Ratio of internal carotid to common carotid PSV",
      "spectral_waveform": "Graphical display of velocity over time"
    }
  },
  "clinical_features": {
    "velocity_criteria": {
      "consensus_criteria": [
        {
          "stenosis": "Normal (<50%)",
          "psv": "<125 cm/s",
          "ica_cca_ratio": "<2.0",
          "plaque": "None or minimal"
        },
        {
          "stenosis": "50-69%",
          "psv": "125-230 cm/s",
          "ica_cca_ratio": "2.0-4.0",
          "plaque": "Visible plaque"
        },
        {
          "stenosis": "\u226570%",
          "psv": ">230 cm/s",
          "ica_cca_ratio": ">4.0",
          "plaque": "Significant luminal narrowing"
        },
        {
          "stenosis": "Near-occlusion",
          "psv": "Variable (may be low or undetectable)",
          "ica_cca_ratio": "Variable",
          "notes": "String sign, color jet visible"
        },
        {
          "stenosis": "Occlusion",
          "psv": "No detectable flow",
          "ica_cca_ratio": "N/A",
          "notes": "Echogenic material filling lumen"
        }
      ]
    },
    "plaque_characteristics": {
      "echogenicity": {
        "echolucent": "Lipid-rich, potentially unstable",
        "echogenic": "Calcified, more stable",
        "heterogeneous": "Mixed, intermediate risk"
      },
      "surface": {
        "smooth": "Lower embolic risk",
        "irregular": "Higher embolic risk",
        "ulcerated": "Highest embolic risk"
      }
    },
    "examination_protocol": [
      "Patient supine, head turned slightly away",
      "Linear high-frequency transducer (7-10 MHz)",
      "Transverse scan to identify vessels",
      "Longitudinal scan for spectral Doppler",
      "Angle correction \u226460\u00b0 for velocity measurement",
      "Sample at maximum stenosis and standard locations"
    ],
    "advantages": [
      "Noninvasive, no radiation",
      "Widely available, portable",
      "Inexpensive",
      "Real-time visualization",
      "Can assess plaque morphology"
    ],
    "limitations": [
      "Operator dependent",
      "Cannot assess intracranial disease",
      "Calcification may obscure imaging",
      "Contralateral disease affects velocities",
      "Body habitus limitations"
    ]
  },
