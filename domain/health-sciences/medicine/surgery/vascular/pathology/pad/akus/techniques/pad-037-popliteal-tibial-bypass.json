        "contralateral_gsv": "Reversed technique",
        "small_saphenous_vein": "If adequate caliber",
        "arm_vein": "Cephalic or basilic vein",
        "spliced_vein": "Composite from multiple sources"
      },
      "prosthetic": {
        "status": "NOT recommended for tibial targets",
        "exception": "None - prosthetic should not be used",
        "reason": "Unacceptable patency rates"
      }
    },
