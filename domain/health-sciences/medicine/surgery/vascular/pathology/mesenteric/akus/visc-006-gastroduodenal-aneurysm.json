  "content": {
    "title": "Gastroduodenal Artery Aneurysm",
    "description": "Rare visceral aneurysm often associated with pancreatitis or as collateral from celiac stenosis.",
    "clinical_features": {
      "epidemiology": "1.5% of visceral aneurysms",
      "common_associations": ["Chronic pancreatitis", "Celiac artery occlusion (as collateral)"],
      "presentation": "May rupture into GI tract causing hematemesis"
    },
    "treatment_approach": {
      "endovascular": "Preferred for pseudoaneurysms from pancreatitis",
      "surgical": "Ligation proximal and distal or resection"
    },
    "clinical_pearl": "GDA aneurysm in setting of celiac occlusion serves as critical collateral; treat celiac stenosis first"
  },
