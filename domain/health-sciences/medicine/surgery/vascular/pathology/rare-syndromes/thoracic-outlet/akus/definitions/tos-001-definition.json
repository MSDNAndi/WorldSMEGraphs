  "content": {
    "statement": "tos-001-definition is a fundamental concept in Health-Sciences Medicine Surgery Vascular Pathology Rare-Syndromes Thoracic-Outlet that provides essential knowledge for understanding related topics and clinical/practical applications.",
    "explanation": {
      "intuition": "Understanding tos-001-definition helps build foundational knowledge. The concept connects to broader principles in the field.",
      "key_insight": "The key aspects of tos-001-definition include core principles, common applications, and integration with related concepts.",
      "technical_details": "Technical details involve specific mechanisms, measurements, and evidence-based applications of tos-001-definition."
    },
    "types": {
      "neurogenic_tos": {
        "frequency": "95%",
        "structure": "Brachial plexus (lower trunk - C8, T1)",
        "presentation": [
          "Upper extremity pain/paresthesias",
          "Worse with arm elevation",
          "Ulnar distribution symptoms",
          "Weakness of intrinsic hand muscles"
        ],
        "diagnosis": [
          "Clinical (provocative tests)",
          "EAST (elevated arm stress test)",
          "Nerve conduction studies often normal"
        ],
        "treatment": [
          "Physical therapy first-line",
          "Scalenectomy/first rib resection if fails"
        ]
      },
      "venous_tos": {
        "frequency": "3-5%",
        "structure": "Subclavian/axillary vein",
        "presentation": [
          "Acute arm swelling",
          "Cyanosis",
          "Pain",
          "Often after exertion ('effort thrombosis')"
        ],
        "diagnosis": [
          "Duplex ultrasound",
          "Venography"
        ],
        "treatment": [
          "Thrombolysis/thrombectomy",
          "Anticoagulation",
          "First rib resection (decompression)"
        ]
      },
      "arterial_tos": {
        "frequency": "1-2%",
        "structure": "Subclavian artery",
        "presentation": [
          "Digital ischemia/embolization",
          "Arm claudication",
          "Subclavian aneurysm",
          "Raynaud's-like symptoms"
        ],
        "diagnosis": [
          "CTA/MRA",
          "Arteriography"
        ],
        "treatment": [
          "First rib resection",
          "Aneurysm repair/bypass if needed",
          "Embolectomy for distal emboli"
        ]
      }
    }
  },
  "clinical_features": {
    "provocative_tests": {
      "adson_test": "Turn head to affected side, extend neck, deep breath - positive if radial pulse diminishes",
      "east_test": "Elevated arm stress test - arms abducted 90\u00b0, repeatedly make fists for 3 min",
      "roos_test": "Same as EAST",
      "costoclavicular_test": "Pull shoulders back and down - positive if reproduces symptoms"
    },
    "anatomic_predispositions": [
      "Cervical rib",
      "Elongated C7 transverse process",
      "Anomalous scalene muscles",
      "Previous clavicle fracture malunion",
      "Repetitive overhead activity"
    ],
    "surgical_approaches": {
      "transaxillary": "First rib resection via axilla - good cosmesis, limited access",
      "supraclavicular": "Scalenectomy \u00b1 first rib resection - better for arterial TOS",
      "infraclavicular": "For venous TOS with residual stenosis - venolysis, patch"
    }
  },
