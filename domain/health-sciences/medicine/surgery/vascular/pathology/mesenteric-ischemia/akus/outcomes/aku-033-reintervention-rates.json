  "endovascular_outcomes": {
    "primary_patency": {
      "definition": "Patency without reintervention",
      "at_1_year": "60-80%",
      "at_3_years": "50-70%",
      "at_5_years": "40-60%",
      "factors_affecting": [
        "Stent type (bare metal vs. covered)",
        "Vessel treated (celiac vs. SMA)",
        "Lesion length",
        "Number of vessels treated",
        "Antiplatelet/anticoagulation compliance"
      ]
    },
    "primary_assisted_patency": {
      "definition": "Patency maintained with prophylactic reintervention for stenosis",
      "at_1_year": "85-90%",
      "note": "Emphasizes importance of surveillance"
    },
    "secondary_patency": {
      "definition": "Patency maintained after reintervention for occlusion",
      "at_1_year": "90-95%",
      "note": "Endovascular often repeatable"
    },
    "restenosis_patterns": {
      "in_stent_restenosis": {
        "incidence": "20-40% at 1 year",
        "mechanism": "Neointimal hyperplasia",
        "timing": "Peak 6-18 months",
        "treatment": "Repeat angioplasty \u00b1 restenting; drug-coated balloon emerging"
      },
      "stent_occlusion": {
        "incidence": "10-15% at 1 year",
        "presentation": "Recurrent CMI symptoms or acute ischemia",
        "management": "Thrombolysis/thrombectomy if acute; recanalization if chronic"
      },
      "edge_stenosis": {
        "incidence": "5-10%",
        "location": "Proximal or distal to stent",
        "treatment": "Extension stenting or angioplasty"
      }
    },
    "stent_type_comparison": {
      "bare_metal_stents": {
        "primary_patency_1_year": "60-70%",
        "advantage": "Cost, availability",
        "disadvantage": "Higher restenosis"
      },
      "covered_stents": {
        "primary_patency_1_year": "75-85%",
        "advantage": "Reduced restenosis",
        "disadvantage": "Cost, concern for branch coverage",
        "note": "Increasingly favored in recent studies"
      }
    },
    "reintervention_rates": {
      "any_reintervention_at_1_year": "20-30%",
      "any_reintervention_at_5_years": "40-50%",
      "multiple_reinterventions": "Common in subset of patients"
    }
  },
  "open_surgical_outcomes": {
    "primary_patency": {
      "at_1_year": "90-95%",
      "at_5_years": "80-90%",
      "at_10_years": "70-80%",
      "note": "Superior durability to endovascular"
    },
    "secondary_patency": {
      "at_5_years": "90-95%",
      "note": "Revision surgery possible but carries significant morbidity"
    },
    "bypass_configurations": {
      "antegrade_supraceliac": {
        "patency": "Highest (85-90% at 5 years)",
        "advantage": "Direct inflow, less kinking",
        "disadvantage": "More complex exposure"
      },
      "retrograde_infrarenal": {
        "patency": "Good (75-85% at 5 years)",
        "advantage": "Easier exposure",
        "disadvantage": "Potential kinking, diseased inflow in some patients"
      }
    },
    "conduit_type": {
      "autologous_vein": {
        "patency": "Excellent (85-90% at 5 years)",
        "preference": "Preferred when available and suitable"
      },
      "prosthetic_ptfe_dacron": {
        "patency": "Good (80-85% at 5 years)",
        "use": "When vein unavailable or unsuitable"
      }
    },
    "failure_modes": {
      "early_failure": {
        "timing": "<30 days",
        "causes": [
          "Technical error",
          "Thrombosis",
          "Conduit issues"
        ],
        "incidence": "1-5%"
      },
      "late_failure": {
        "timing": ">30 days",
        "causes": [
          "Progression of disease",
          "Neointimal hyperplasia",
          "Graft degeneration"
        ],
        "management": "Revision surgery or endovascular salvage"
      }
    },
    "reintervention_rates": {
      "any_reintervention_at_5_years": "10-20%",
      "morbidity_of_redo": "Higher than index procedure"
    }
  },
  "comparison_endovascular_vs_open": {
    "durability": {
      "open": "Superior primary patency",
      "endovascular": "Higher reintervention but often feasible"
    },
    "perioperative_risk": {
      "open": "Higher 30-day mortality (3-8%)",
      "endovascular": "Lower 30-day mortality (0-2%)"
    },
    "symptom_relief": {
      "open": "90-95% initial success",
      "endovascular": "85-90% initial success"
    },
    "total_interventions_over_time": {
      "endovascular": "Often multiple (average 1.5-2 procedures per patient at 5 years)",
      "open": "Usually one if successful"
    },
    "patient_selection_factors": {
      "favor_endovascular": [
        "High surgical risk",
        "Single-vessel disease",
        "Focal lesions",
        "Limited life expectancy",
        "Patient preference"
      ],
      "favor_open": [
        "Good surgical candidate",
        "Multi-vessel disease",
        "Long/complex lesions",
        "Prior failed endovascular",
        "Long life expectancy"
      ]
    }
  },
  "acute_mesenteric_ischemia_outcomes": {
    "revascularization_durability": {
      "note": "Less studied than CMI; short-term survival is primary concern",
      "early_thrombosis_risk": "Higher due to low-flow state, bowel edema",
      "anticoagulation": "Critical in acute setting"
    },
    "second_look_reintervention": {
      "planned_relook": "24-48 hours after initial surgery",
      "reintervention_rate": "30-50% require additional bowel resection",
      "revascularization_revision": "Rare if initial procedure successful"
    }
  },
  "predictors_of_reintervention": {
    "patient_factors": [
      "Continued smoking",
      "Diabetes mellitus",
      "Hypercoagulable states",
      "Poor antiplatelet/anticoagulation compliance"
    ],
    "anatomic_factors": [
      "Long lesion length (>20mm for stents)",
      "Small vessel diameter",
      "Severe calcification",
      "Residual stenosis post-procedure"
    ],
    "technical_factors": [
      "Incomplete revascularization",
      "Stent undersizing",
      "Poor inflow or outflow"
    ]
  },
  "clinical_pearls": [
    {
      "pearl": "Endovascular revascularization for CMI should be viewed as a 'chronic therapy' requiring surveillance and likely reintervention",
      "implication": "Counsel patients about surveillance expectations and potential for repeat procedures"
    },
    {
      "pearl": "Recurrent symptoms after mesenteric stenting should prompt urgent imaging\u2014restenosis can progress rapidly to occlusion",
      "implication": "Do not dismiss recurrent postprandial pain as unrelated; low threshold for duplex or CTA"
    },
    {
      "pearl": "Covered stents may offer improved patency over bare metal stents for mesenteric revascularization",
      "implication": "Consider covered stents as first-line for SMA stenting"
    },
    {
      "pearl": "Open bypass remains the most durable option for good-risk patients with complex lesions or multiple endovascular failures",
      "implication": "Don't exhaust endovascular options in patients who are good surgical candidates"
    }
  ],
