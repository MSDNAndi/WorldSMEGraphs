  "content": {
    "statement": "aku-022-endovascular-treatment is a fundamental concept in Health-Sciences Medicine Surgery Vascular Pathology Mesenteric-Ischemia that provides essential knowledge for understanding related topics and clinical/practical applications.",
    "explanation": {
      "intuition": "Understanding aku-022-endovascular-treatment helps build foundational knowledge. The concept connects to broader principles in the field.",
      "key_insight": "The key aspects of aku-022-endovascular-treatment include core principles, common applications, and integration with related concepts.",
      "technical_details": "Technical details involve specific mechanisms, measurements, and evidence-based applications of aku-022-endovascular-treatment."
    },
    "definitions_glossary": {
      "concept": "A fundamental idea or notion in the domain",
      "property": "A characteristic or attribute of an entity",
      "relationship": "A connection or association between concepts"
    }
  },
  "techniques": {
    "for_acute_occlusion": {
      "aspiration_thrombectomy": {
        "technique": "Large-bore catheter aspiration of thrombus",
        "access": "Femoral or brachial",
        "devices": [
          "Penumbra",
          "AngioJet",
          "Indigo"
        ],
        "advantages": "Rapid clot removal, less bleeding than lysis",
        "limitations": "May not remove organized thrombus"
      },
      "pharmacomechanical_thrombectomy": {
        "technique": "Combination of mechanical disruption and thrombolysis",
        "devices": [
          "AngioJet with tPA",
          "Trellis"
        ],
        "advantages": "Synergistic clot removal",
        "duration": "Single session preferred"
      },
      "catheter_directed_thrombolysis": {
        "technique": "Infusion catheter positioned in thrombus",
        "agents": [
          "tPA 0.5-1 mg/hr",
          "Reteplase",
          "Urokinase"
        ],
        "duration": "12-24 hours with serial angiography",
        "advantages": "Can treat extensive disease",
        "disadvantages": "Time-consuming, bleeding risk, delays surgery"
      },
      "balloon_angioplasty_plus_stenting": {
        "role": "For underlying stenosis after thrombus removal",
        "approach": "Address residual stenosis to prevent rethrombosis"
      }
    },
    "for_chronic_disease": {
      "primary_stenting": {
        "indication": "CMI with ostial/proximal SMA stenosis",
        "technique": [
          "Femoral or brachial access",
          "Selective SMA catheterization",
          "Wire across stenosis",
          "Predilation if needed",
          "Balloon-expandable stent deployment",
          "Post-dilation"
        ],
        "stent_type": {
          "ostial_lesions": "Balloon-expandable (superior radial force)",
          "distal_lesions": "Self-expanding may be used"
        },
        "stent_diameter": "6-8 mm typically",
        "stent_length": "Cover lesion with minimal protrusion into aorta"
      },
      "target_vessels": {
        "SMA": "Primary target for CMI",
        "celiac": "Added if severely diseased",
        "both": "Two-vessel stenting for multi-vessel disease"
      }
    }
  },
  "patient_selection": {
    "ideal_for_endovascular": [
      "Chronic mesenteric ischemia (elective)",
      "Acute occlusion with stable vital signs",
      "No peritonitis",
      "Short segment stenosis/occlusion",
      "High surgical risk patient"
    ],
    "prefer_open_surgery": [
      "Peritonitis or signs of bowel necrosis",
      "Hemodynamic instability",
      "Need to assess bowel viability",
      "Failed endovascular attempt",
      "Extensive chronic occlusion"
    ],
    "hybrid_approach": {
      "indication": "Endovascular revascularization followed by laparoscopy or laparotomy for bowel assessment",
      "advantages": "Minimally invasive revascularization with bowel evaluation",
      "setting": "Requires hybrid OR or coordinated team"
    }
  },
  "outcomes": {
    "technical_success": {
      "chronic_stenosis": "90-95%",
      "acute_occlusion": "70-90%"
    },
    "clinical_success": {
      "symptom_relief_CMI": "80-95%",
      "mortality_AMI": "Varies 25-50% (depends on patient selection)"
    },
    "patency": {
      "primary_patency_1yr": "70-85%",
      "primary_patency_3yr": "50-70%",
      "assisted_patency_3yr": "80-90%",
      "comparison": "Lower than open bypass but acceptable with surveillance"
    },
    "complications": {
      "access_site": "5-10% (hematoma, pseudoaneurysm)",
      "embolization": "5-10%",
      "dissection": "2-5%",
      "stent_thrombosis": "5-10%",
      "renal_dysfunction": "5-10% (contrast nephropathy)"
    }
  },
  "advantages_vs_open": [
    "Less invasive, faster recovery",
    "Can be performed under local anesthesia",
    "Lower perioperative morbidity",
    "Repeatable if needed",
    "May be done in high-risk surgical patients"
  ],
  "disadvantages_vs_open": [
    "Cannot assess bowel viability",
    "Lower long-term patency",
    "Risk of embolization during manipulation",
    "Requires surveillance and reintervention",
    "Limited role if bowel necrosis present"
  ],
  "surveillance": {
    "protocol": {
      "post_procedure": "Duplex ultrasound at 1 month",
      "first_year": "Duplex every 3-6 months",
      "subsequent": "Duplex annually",
      "indications_for_reintervention": [
        "In-stent restenosis >70%",
        "Recurrent symptoms",
        "PSV >400 cm/s or ratio >4:1"
      ]
    }
  },
