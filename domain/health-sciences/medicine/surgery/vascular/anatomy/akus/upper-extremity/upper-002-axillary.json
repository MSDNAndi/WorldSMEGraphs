  "content": {
    "title": "Axillary Artery Anatomy",
    "description": "Continuation of subclavian artery from lateral border of first rib to lower border of teres major, divided into three parts by pectoralis minor muscle.",
    "boundaries": {
      "proximal": "Lateral border of first rib (from subclavian)",
      "distal": "Lower border of teres major (becomes brachial)"
    },
    "divisions": {
      "first_part": "Above pectoralis minor",
      "second_part": "Behind pectoralis minor",
      "third_part": "Below pectoralis minor"
    },
    "branches": {
      "first_part": "Superior thoracic (1 branch)",
      "second_part": "Thoracoacromial, lateral thoracic (2 branches)",
      "third_part": "Subscapular, anterior and posterior circumflex humeral (3 branches)"
    },
    "relations": {
      "axillary_vein": "Medial and slightly anterior",
      "brachial_plexus": "Cords surround second part (lateral, posterior, medial)",
      "pectoralis_minor": "Key landmark dividing artery"
    },
    "surgical_exposure": {
      "infraclavicular": "Standard approach",
      "transaxillary": "Limited approach",
      "landmarks": "Deltopectoral groove, pectoralis major"
    },
    "clinical_relevance": "Important for dialysis access, extra-anatomic bypass, trauma"
  },
