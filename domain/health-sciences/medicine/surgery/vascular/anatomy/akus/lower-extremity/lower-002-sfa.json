  "content": {
    "title": "Superficial Femoral Artery Anatomy",
    "description": "The major arterial conduit of the thigh, continuation of CFA, and the most common site of symptomatic atherosclerotic occlusive disease.",
    "origin": "Common femoral artery bifurcation",
    "course": "Runs through adductor (Hunter's) canal in anteromedial thigh",
    "termination": "Becomes popliteal artery as it passes through adductor hiatus",
    "anatomic_segments": {
      "proximal": "Origin to 5cm below inguinal ligament",
      "mid": "Within adductor canal",
      "distal": "Near adductor hiatus"
    },
    "adductor_canal": {
      "also_known_as": "Hunter's canal, subsartorial canal",
      "boundaries": {
        "anterior": "Sartorius",
        "lateral": "Vastus medialis",
        "posterior": "Adductor longus and magnus"
      },
      "contents": ["SFA", "Femoral vein", "Saphenous nerve"]
    },
    "branches": "Minimal - primarily muscular branches",
    "disease_predilection": {
      "most_common_site": "Adductor canal (repetitive flexion stress)",
      "presentations": ["Claudication (most common)", "Critical limb ischemia"],
      "collateral": "Profunda femoris reconstitutes popliteal via genicular network"
    }
  },
