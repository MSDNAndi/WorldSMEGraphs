  "content": {
    "title": "Common Femoral Artery Anatomy",
    "description": "Critical vessel serving as the main arterial access site and key surgical landmark in lower extremity vascular surgery.",
    "origin": "Continuation of external iliac artery below inguinal ligament",
    "termination": "Bifurcates into superficial and deep (profunda) femoral arteries",
    "location": {
      "surface_landmark": "Femoral triangle, 2cm below inguinal ligament",
      "depth": "Superficial, easily palpable",
      "relation_to_femoral_head": "Overlies femoral head on fluoroscopy"
    },
    "femoral_triangle": {
      "boundaries": {
        "superior": "Inguinal ligament",
        "medial": "Adductor longus",
        "lateral": "Sartorius"
      },
      "contents_lateral_to_medial": "NAVEL (Nerve, Artery, Vein, Empty space, Lymphatics)"
    },
    "branches": {
      "superficial": ["Superficial epigastric", "Superficial circumflex iliac", "Superficial external pudendal"],
      "deep": ["Profunda femoris (largest)"]
    },
    "surgical_significance": {
      "access": "Standard site for catheter-based interventions",
      "endarterectomy": "Common site of atherosclerotic disease",
      "bypass": "Common origin or target for bypass grafts"
    }
  },
