  "content": {
    "title": "Popliteal Artery Anatomy",
    "description": "Continuation of SFA through the popliteal fossa, giving rise to genicular branches and terminating as the tibial trifurcation.",
    "origin": "At adductor hiatus (continuation of SFA)",
    "termination": "At lower border of popliteus muscle (tibial trifurcation)",
    "segments": {
      "P1": "Above knee, from adductor hiatus to joint line",
      "P2": "At knee joint (behind patella)",
      "P3": "Below knee, to trifurcation"
    },
    "popliteal_fossa": {
      "boundaries": {
        "superolateral": "Biceps femoris",
        "superomedial": "Semimembranosus and semitendinosus",
        "inferolateral": "Lateral head of gastrocnemius",
        "inferomedial": "Medial head of gastrocnemius"
      },
      "contents": "Artery deepest, vein superficial to artery, tibial nerve most superficial"
    },
    "branches": {
      "genicular": ["Superior lateral and medial", "Inferior lateral and medial", "Middle genicular"],
      "muscular": "To gastrocnemius and soleus"
    },
    "termination": "Anterior tibial (first branch), then tibioperoneal trunk â†’ posterior tibial and peroneal"
  },
