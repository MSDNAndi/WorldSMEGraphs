  "content": {
    "title": "Fenestrated and Branched Endovascular Aortic Repair",
    "description": "Complex endovascular repair using custom or physician-modified grafts with fenestrations or branches to incorporate visceral and renal vessels.",
    "types": {
      "fenestrated": {
        "mechanism": "Holes in graft fabric aligned with branch vessels",
        "sealing": "Covered stent from fenestration into branch vessel",
        "applications": "Juxtarenal and suprarenal AAA"
      },
      "branched": {
        "mechanism": "Directional branches built into graft",
        "sealing": "Bridging stent from branch into vessel",
        "applications": "Thoracoabdominal aneurysm (TAAA)"
      }
    },
    "target_vessels": {
      "celiac": "Often scallop or fenestration",
      "sma": "Fenestration or branch",
      "renal": "Fenestrations most common"
    },
    "planning": {
      "imaging": "High-resolution CTA with 3D reconstruction",
      "clock_positions": "Branch vessel positions mapped to graft design",
      "manufacturing_time": "4-8 weeks for custom devices",
      "pmeg": "Physician-modified endografts for urgent cases"
    },
    "procedural_considerations": {
      "access": "Often bilateral femoral + upper extremity",
      "radiation": "Prolonged fluoroscopy time",
      "contrast": "High volumes, consider CO2 adjunct",
      "renal_protection": "Minimize ischemia time during stenting"
    },
    "outcomes": {
      "technical_success": ">90%",
      "target_vessel_patency": ">90% at 1 year"
    }
  },
