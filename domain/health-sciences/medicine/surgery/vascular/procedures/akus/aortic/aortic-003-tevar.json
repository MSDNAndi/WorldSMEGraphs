  "content": {
    "title": "Thoracic Endovascular Aortic Repair (TEVAR)",
    "description": "Endovascular treatment of thoracic aortic pathology using covered stent grafts, with applications in aneurysm, dissection, trauma, and intramural hematoma.",
    "indications": {
      "aneurysm": ["Descending thoracic aneurysm â‰¥5.5-6cm", "Rapid expansion", "Symptomatic"],
      "dissection": ["Complicated Type B dissection", "Uncomplicated Type B (selective, INSTEAD-XL data)"],
      "trauma": ["Blunt aortic injury (preferred approach)", "Pseudoaneurysm"],
      "other": ["Penetrating aortic ulcer", "Intramural hematoma with progression"]
    },
    "landing_zones": {
      "zone_0": "Ascending aorta (requires ascending repair capability)",
      "zone_1": "Between innominate and LCCA",
      "zone_2": "Between LCCA and left subclavian",
      "zone_3": "Proximal descending (0-2cm from LSCA)",
      "zone_4": "Mid-descending (>2cm from LSCA)"
    },
    "subclavian_coverage": {
      "considerations": "Left vertebral dominance, prior CABG with LIMA, left arm dialysis access",
      "management": "Revascularization (carotid-subclavian bypass or transposition) if risk factors present"
    },
    "spinal_cord_protection": {
      "risk_factors": "Long coverage, prior AAA repair, LSA coverage, renal insufficiency",
      "strategies": ["CSF drainage", "Mean arterial pressure augmentation", "Staged procedures"]
    },
    "outcomes": {
      "technical_success": ">95%",
      "spinal_cord_ischemia": "3-8%"
    }
  },
  "relationships": {
    "related_to": ["vasc:aortic:dissection", "vasc:aortic:descending-aneurysm"],
