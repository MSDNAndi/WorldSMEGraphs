  "content": {
    "title": "Covered Stent Grafts",
    "description": "Stents with PTFE or Dacron covering that creates a barrier between blood flow and vessel wall, used for aneurysm exclusion, fistula, trauma, and in-stent restenosis.",
    "covering_materials": {
      "ePTFE": "Most common, encapsulated or external",
      "dacron": "Less common in peripheral applications"
    },
    "indications": {
      "aneurysm": "Peripheral aneurysm exclusion",
      "trauma": "Arterial injury, pseudoaneurysm",
      "fistula": "AV fistula, aortoenteric fistula",
      "isr": "In-stent restenosis (SFA)",
      "rupture": "Vessel perforation or rupture"
    },
    "sizing": "Typically oversized 10-20% for adequate seal zone",
    "specific_devices": {
      "viabahn": "Self-expanding, flexible, for SFA",
      "fluency": "Self-expanding for various applications",
      "ivascular": "Balloon-expandable for iliac"
    },
    "considerations": [
      "Coverage of branch vessels",
      "Seal zone requirements",
      "Flexibility needs",
      "Long-term patency",
      "Higher cost than bare metal"
    ]
  },
