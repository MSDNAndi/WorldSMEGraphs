  "content": {
    "title": "Brachial Artery Access",
    "description": "Upper extremity arterial access via the brachial artery, used when femoral access is not possible or for interventions requiring antegrade aortic approach.",
    "indications": [
      "Severe aortoiliac disease preventing femoral access",
      "Bilateral groin infection or scarring",
      "Combined upper and lower extremity procedures",
      "Antegrade approach needed",
      "Arch vessel intervention"
    ],
    "anatomy": {
      "location": "Antecubital fossa, medial to biceps tendon",
      "relationships": "Median nerve medial, biceps lateral",
      "size": "Typically 3-5mm diameter"
    },
    "technique": {
      "positioning": "Arm abducted on arm board",
      "access": "Ultrasound-guided preferred",
      "sheath_size": "Usually limited to 5-6Fr due to vessel size"
    },
    "complications": {
      "hematoma": "Higher risk than femoral",
      "thrombosis": "Higher risk in small caliber vessels",
      "nerve_injury": "Median nerve proximity",
      "compartment_syndrome": "Rare but serious"
    },
    "advantages": ["Superficial, easy to compress", "Avoids groin complications"],
    "disadvantages": ["Size limitations", "Higher thrombosis rate", "Limited sheath size"]
  },
  "relationships": {
    "related_to": ["vasc:procedures:access-001-femoral-retrograde", "vasc:procedures:access-003-radial"],
