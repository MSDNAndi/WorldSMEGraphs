  "content": {
    "title": "Tibial/Pedal Artery Access",
    "description": "Retrograde access via tibiopedal arteries (anterior tibial, posterior tibial, peroneal, or dorsalis pedis) for complex infrainguinal interventions.",
    "indications": [
      "Failed antegrade tibial intervention",
      "CTO extending to ankle",
      "Direct angiosome revascularization",
      "Very distal disease"
    ],
    "target_vessels": {
      "anterior_tibial": "Lateral ankle, transitions to DP",
      "posterior_tibial": "Behind medial malleolus",
      "dorsalis_pedis": "Dorsum of foot",
      "peroneal": "Less common access site"
    },
    "technique": {
      "guidance": "Ultrasound essential",
      "sheath": "4Fr maximum, often sheathless",
      "wire": "0.014-0.018 inch",
      "pedal_arch": "May use for loop technique"
    },
    "SAFARI_technique": "Subintimal Arterial Flossing with Antegrade-Retrograde Intervention - meeting wires in CTO to establish through-and-through access",
    "complications": {
      "vessel_loss": "Small caliber, higher thrombosis risk",
      "spasm": "Treat with vasodilators",
      "dissection": "May compromise limited runoff"
    }
  },
