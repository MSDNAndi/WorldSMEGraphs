  "content": {
    "title": "Pharmacomechanical Catheter-Directed Thrombolysis",
    "description": "Combined approach using thrombolytic drugs delivered through specialized catheters with or without mechanical adjuncts to accelerate thrombus dissolution.",
    "components": {
      "thrombolytic_agents": ["tPA (alteplase)", "Reteplase", "Tenecteplase"],
      "delivery_methods": ["Multi-sidehole catheter infusion", "Ultrasound-assisted (EKOS)", "Power-pulse spray (AngioJet)"],
      "mechanical_adjuncts": ["Balloon maceration", "Rheolytic thrombectomy", "Aspiration"]
    },
    "EKOS_ultrasound_assisted": {
      "mechanism": "Ultrasound loosens fibrin matrix, enhances drug penetration",
      "advantages": "May allow lower lytic dose, faster resolution",
      "evidence": "ATTRACT trial data"
    },
    "protocol_example": {
      "access": "Establish access beyond thrombus if possible",
      "catheter_placement": "Infusion catheter spanning thrombus",
      "tPA_dose": "0.5-1 mg/hr typical (varies by protocol)",
      "check_intervals": "Reimage at 12-24 hours",
      "endpoints": "Complete lysis or conversion to mechanical therapy"
    },
    "monitoring": ["Fibrinogen levels", "Clinical exam", "Repeat imaging"],
    "complications": {
      "bleeding": "Major bleeding 5-10%",
      "ich": "Intracranial hemorrhage 0.5-1%"
    }
  },
