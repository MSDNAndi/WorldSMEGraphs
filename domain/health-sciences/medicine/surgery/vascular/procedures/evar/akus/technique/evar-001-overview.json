  "content": {
    "statement": "evar-001-overview is a fundamental concept in Health-Sciences Medicine Surgery Vascular Procedures Evar that provides essential knowledge for understanding related topics and clinical/practical applications.",
    "explanation": {
      "intuition": "Understanding evar-001-overview helps build foundational knowledge. The concept connects to broader principles in the field.",
      "key_insight": "The key aspects of evar-001-overview include core principles, common applications, and integration with related concepts.",
      "technical_details": "Technical details involve specific mechanisms, measurements, and evidence-based applications of evar-001-overview."
    },
    "definitions_glossary": {
      "stent_graft": "Self-expanding or balloon-expandable covered stent to exclude aneurysm",
      "landing_zone": "Normal aortic segment where graft seals, above and below aneurysm",
      "proximal_neck": "Segment of normal aorta between renal arteries and aneurysm",
      "modular_design": "Multi-piece device assembled in situ (main body + limbs)",
      "fenestrated_evar": "Device with holes for visceral arteries in difficult anatomy",
      "branched_evar": "Device with side branches for visceral/renal arteries"
    }
  },
  "clinical_features": {
    "indications": {
      "standard": [
        "AAA \u22655.5 cm (men) or \u22655.0 cm (women)",
        "AAA \u22654.5 cm with rapid expansion (>1 cm/year)",
        "Symptomatic AAA (any size)",
        "Suitable anatomy per device IFU"
      ],
      "expanded": [
        "Ruptured AAA (if stable enough for CT)",
        "Complex anatomy with fenestrated/branched devices",
        "High surgical risk patients"
      ]
    },
    "anatomic_requirements": {
      "proximal_neck": {
        "length": "\u226510-15 mm (device dependent)",
        "diameter": "18-32 mm (varies by device)",
        "angulation": "<60\u00b0 suprarenal, <45\u00b0 infrarenal",
        "thrombus_calcification": "Minimal (<50% circumference)"
      },
      "access_vessels": {
        "minimum_diameter": "\u22657 mm typically",
        "tortuosity": "Navigable with stiff wire support",
        "calcification": "No significant obstructing lesions"
      },
      "iliac_landing": {
        "length": "\u226515 mm in common iliac or external iliac with hypogastric coverage"
      }
    },
    "outcomes": {
      "perioperative_mortality": "1-2% (vs 3-5% open repair)",
      "technical_success": "95-99%",
      "30_day_major_complications": "10-15%",
      "5_year_reintervention": "15-25%",
      "5_year_aneurysm_related_mortality": "3-5%",
      "endoleak_rate": "20-40% at some point"
    },
    "surveillance_protocol": {
      "first_month": "CT angiography at 1 month",
      "first_year": "CT at 6 and 12 months",
      "annual": "CT or duplex ultrasound annually",
      "indications_for_intervention": [
        "Type I or III endoleak",
        "Type II endoleak with sac growth >5 mm",
        "Device migration >10 mm",
        "Limb thrombosis"
      ]
    }
  },
