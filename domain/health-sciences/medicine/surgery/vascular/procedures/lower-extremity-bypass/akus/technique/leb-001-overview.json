  "content": {
    "statement": "leb-001-overview is a fundamental concept in Health-Sciences Medicine Surgery Vascular Procedures Lower-Extremity-Bypass that provides essential knowledge for understanding related topics and clinical/practical applications.",
    "explanation": {
      "intuition": "Understanding leb-001-overview helps build foundational knowledge. The concept connects to broader principles in the field.",
      "key_insight": "The key aspects of leb-001-overview include core principles, common applications, and integration with related concepts.",
      "technical_details": "Technical details involve specific mechanisms, measurements, and evidence-based applications of leb-001-overview."
    },
    "definitions_glossary": {
      "conduit": "The vessel or graft material connecting inflow to outflow",
      "great_saphenous_vein": "Superficial leg vein running from ankle to groin",
      "infrainguinal": "Below the inguinal ligament (below groin)",
      "patency": "Openness of the graft or vessel, typically measured as proportion still open at time X",
      "reversed_vein": "Vein reversed in orientation so valves allow flow",
      "in_situ_bypass": "Vein left in place with valves destroyed to allow arterial flow"
    }
  },
  "clinical_features": {
    "indications": {
      "critical_limb_ischemia": {
        "rest_pain": "Fontaine III, Rutherford 4",
        "tissue_loss": "Fontaine IV, Rutherford 5-6"
      },
      "lifestyle_limiting_claudication": "After failed exercise/medical therapy, Rutherford 2-3",
      "acute_limb_ischemia": "When thrombectomy alone insufficient"
    },
    "conduit_options": {
      "autologous_vein": {
        "great_saphenous_vein": {
          "quality": "Best conduit, 70-80% 5-year patency",
          "minimum_diameter": "\u22653.0-3.5 mm"
        },
        "small_saphenous_vein": "Alternative when GSV unavailable",
        "arm_veins": "Cephalic, basilic - second choice after GSV",
        "spliced_vein": "Combine segments to achieve adequate length"
      },
      "prosthetic": {
        "ptfe": "Polytetrafluoroethylene, common for above-knee",
        "dacron": "Typically for larger diameter (aorto-femoral)",
        "patency": "50-60% AK 5-year, 30-40% BK 5-year"
      }
    },
    "configurations": [
      {
        "bypass": "Femoral-popliteal above-knee",
        "inflow": "Common femoral artery",
        "outflow": "Above-knee popliteal artery",
        "conduit_options": "GSV preferred, prosthetic acceptable"
      },
      {
        "bypass": "Femoral-popliteal below-knee",
        "inflow": "Common femoral artery",
        "outflow": "Below-knee popliteal artery",
        "conduit_options": "GSV strongly preferred"
      },
      {
        "bypass": "Femoral-tibial/peroneal",
        "inflow": "Common femoral artery",
        "outflow": "Tibial or peroneal artery",
        "conduit_options": "Vein only - prosthetic poor results"
      }
    ],
    "postoperative_surveillance": {
      "schedule": "1, 3, 6, 12 months, then annually",
      "modality": "Duplex ultrasound with velocity measurement",
      "indication_for_revision": "PSV >300 cm/s or PSV ratio >3.5 suggests stenosis"
    }
  },
