  "clinical_features": {
    "indications": {
      "aneurysm": "Descending thoracic aneurysm \u22655.5-6.0 cm or rapid growth",
      "dissection": "Complicated acute Type B dissection (malperfusion, rupture, refractory pain/HTN)",
      "trauma": "Blunt thoracic aortic injury (BTAI)",
      "ulcer": "Symptomatic penetrating aortic ulcer (PAU)",
      "other": [
        "Intramural hematoma with concerning features",
        "Chronic post-dissection aneurysmal degeneration"
      ]
    },
    "contraindications": {
      "anatomic": [
        "Inadequate landing zones",
        "Excessive tortuosity/calcification",
        "Small access vessels"
      ],
      "relative": [
        "Connective tissue disorders (Marfan, LDS, EDS)",
        "Arch involvement requiring complex repair"
      ]
    },
    "complications": {
      "perioperative": [
        "Stroke (2-4%)",
        "Spinal cord ischemia (2-8%)",
        "Access complications",
        "Endoleak"
      ],
      "late": [
        "Retrograde Type A dissection",
        "Device migration",
        "Late endoleak",
        "Aortic neck degeneration"
      ]
    },
    "spinal_cord_protection": {
      "risk_factors": [
        "Long coverage (>20cm)",
        "Prior AAA repair",
        "Coverage below T8",
        "Hypotension"
      ],
      "strategies": [
        "CSF drainage",
        "MAP augmentation",
        "Staged procedures",
        "Left subclavian revascularization"
      ]
    },
    "outcomes": {
      "mortality": "2-5% elective (vs 8-15% open repair)",
      "paraplegia": "2-8% (higher with extensive coverage)",
      "surveillance": "CTA at 1, 6, 12 months then annually"
